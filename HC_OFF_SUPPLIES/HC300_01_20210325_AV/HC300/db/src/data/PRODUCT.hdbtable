COLUMN TABLE "HC::PRODUCT"(
	"PRODUCT_ID" NVARCHAR(10) DEFAULT '' NOT NULL,
	"PRODUCT_TEXT" VARCHAR(30) DEFAULT '',
	"PRODUCT_GROUP" VARCHAR(20) DEFAULT '',
	"BASIS_PRICE" DECIMAL(17, 2) DEFAULT 0 NOT NULL,
	"CURRENCY" VARCHAR(9) DEFAULT '',
	"DIVISION" VARCHAR(20) DEFAULT '',
	PRIMARY KEY (
		"PRODUCT_ID"
	)
)
;
