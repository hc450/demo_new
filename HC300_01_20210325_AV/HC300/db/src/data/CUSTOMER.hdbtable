COLUMN TABLE "HC::CUSTOMER"(
	"CUSTOMER_ID" NVARCHAR(10) DEFAULT '' NOT NULL,
	"CUSTOMER_TEXT" VARCHAR(30) DEFAULT '',
	"COUNTRY" VARCHAR(5) DEFAULT '',
	"REGION" VARCHAR(5) DEFAULT '',
	"LOCATION" VARCHAR(30) DEFAULT '',
	"DISCOUNT_GROUP_ID" VARCHAR(4),
	PRIMARY KEY (
		"CUSTOMER_ID"
	)
)
;